#!/usr/bin/env ruby
require 'date'

base_url = "http://data.githubarchive.org"
start_date  = Date.new( 2011, 02, 12 )
start_date = Date.new( 2013, 02, 01 )
end_date = Date.today

(start_date..end_date).each do |d|
  (0..23).each do |h|
   puts "#{base_url}/#{d.strftime("%Y-%m-%d")}-#{h}.json.gz"
  end
end
